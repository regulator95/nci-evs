{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=5.6",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/admin_toolbar": "^2.0",
        "drupal/asset_injector": "^2.4",
        "drupal/autologout": "^1.0",
        "drupal/backup_migrate": "^4.0",
        "drupal/bootstrap_barrio": "^4.22",
        "drupal/config_sync": "^2.0@beta",
        "drupal/console": "^1.0.2",
        "drupal/core": "^8.7.0",
        "drupal/devel": "^2.1",
        "drupal/diff": "^1.0@RC",
        "drupal/exclude_node_title": "^1.0@beta",
        "drupal/extlink": "^1.1",
        "drupal/google_analytics": "^3.0",
        "drupal/imce": "^1.7",
        "drupal/ldap": "^3.0@beta",
        "drupal/metatag": "^1.8",
        "drupal/module_filter": "^3.1",
        "drupal/module_missing_message_fixer": "^1.0",
        "drupal/pathauto": "^1.4",
        "drupal/redirect": "^1.3",
        "drupal/redirect_after_login": "^2.5",
        "drupal/security_review": "1.x-dev",
        "drupal/twig_xdebug": "^1.1",
        "drupal/views_fieldsets": "^3.3",
        "drupal/xmlsitemap": "^1.0@alpha",
        "drush/drush": "^9.6",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "^8.7.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
